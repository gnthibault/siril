msgstr "Εκκίνηση δέσμης ενεργειών %s\n"

#: src/core/command_line_processor.c:366
msgid "Command execution failed.\n"
msgstr "Η εκτέλεση της εντολής απέτυχε.\n"

#: src/core/command_line_processor.c:412
#, c-format
msgid "Loading sequence `%s' failed.\n"
msgstr "Η φόρτωση της ακολουθίας `%s' απέτυχε.\n"

#: src/core/initfile.c:56
#, c-format
msgid "Loading init file: '%s'\n"
msgstr "Γίνεται φόρτωση του αρχείου init: '%s'\n"

#: src/core/OS_utils.c:222
msgid "Error while computing available free disk space.\n"
msgstr ""
"Σφάλμα κατά τον υπολογισμό του διαθέσιμου ελεύθερου χώρου στον δίσκο.\n"
"\n"

#: src/core/OS_utils.c:226
msgid "Error in requested space disk.\n"
msgstr "Σφάλμα στον απαιτούμενο χώρο δίσκου.\n"

#: src/core/OS_utils.c:237
#, c-format
msgid ""
"Compression enabled: There may no be enough free disk space to perform this "
"operation: %s available for %s needed (missing %s)\n"
msgstr ""
