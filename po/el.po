#, c-format
msgid "Usage: %s\n"
msgstr "Χρήση: %s\n"

#: src/core/command_line_processor.c:96
#, c-format
msgid "This command cannot be used in a script: %s\n"
msgstr "Αυτή η εντολή δεν μπορεί να χρησιμοποιηθεί σε δέσμη ενεργειών: %s\n"

#. process the command
#: src/core/command_line_processor.c:102
#, c-format
msgid "Running command: %s\n"
msgstr "Γίνεται εκτέλεση της εντολής: %s\n"

#: src/core/command_line_processor.c:130
#, c-format
msgid "Processing line %d: %s"
msgstr "Γίνεται επεξεργασία της γραμμής %d: %s"

#: src/core/command_line_processor.c:220
msgid ""
"The \"requires\" command is missing at the top of the script file. This "
"command is needed to check script compatibility.\n"
msgstr ""
"Η εντολή \"requires\" λείπει στο επάνω μέρος του αρχείου δέσμης ενεργειών. "
"Αυτή η εντολή είναι απαραίτητη για τον έλεγχο της συμβατότητας δέσμης "
"ενεργειών.\n"

#: src/core/command_line_processor.c:228
#, c-format
