msgid "GZIP2"
msgstr "GZIP2"

#: src/gui/siril3.glade:18597
msgid "Method"
msgstr "Способ"

#: src/gui/siril3.glade:18611
msgid "Quantization level"
msgstr "Уровень квантования"

#: src/gui/siril3.glade:18624
